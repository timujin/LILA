script/source = "extends GridContainer

onready var karma_item = preload(\"res://Autoload/WorldspaceHUD/Character/KarmaItem.tscn\")

func render_category(category:String):
	var items = []
	for item in CharacterData.karma.karma:
		var icategory = item.species.category
		if icategory == category:
			items.append(item)
			
	for item in items:
		var renderer = karma_item.instance()
		add_child(renderer)
		renderer.render(item)
	
	\"\"\"
	for key in $\"/root/CharacterData\".karma.keys():
		var item = KarmaItem.load_by_id(key)
		if item.category == category:
			items.append(key)
	for id in items:
		var renderer = karma_item.instance()
		add_child(renderer)
		renderer.render_id(id)
	\"\"\"
		
func _ready():
	pass#render_category(\"Misc\")
		
"

